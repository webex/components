{
  "name": "@webex/components",
  "version": "0.0.1",
  "main": "dist/webexComponents.cjs.js",
  "module": "dist/webexComponents.esm.js",
  "scripts": {
    "build": "rollup -c",
    "prettier": "prettier \"src/**/!(static)/**/*.*(js|jsx)\"",
    "prettier-write": "prettier \"src/**/!(static)/**/*.*(js|jsx)\" --write",
    "start": "start-storybook -p 8080 -h localhost",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@momentum-ui/react": "^20.1.1"
  },
  "author": "devsupport@webex.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.1.9",
    "@webex/eslint-config-base": "^0.8.0",
    "@webex/eslint-config-react": "^0.8.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-export-default-from"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/.storybook/setupTests.js"
    ],
    "snapshotSerializers": ["enzyme-to-json/serializer"]
  }
}
